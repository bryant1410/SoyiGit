<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="cuteEditText">
        <attr name="text_minLength" format="integer" />
        <attr name="text_maxLength" format="integer" />
        <attr name="text_hint" format="string" />
        <attr name="text_anim_Type" format="string" />
    </declare-styleable>

    <declare-styleable name="labelImageView">
        <attr name="text_color" format="color" />
        <attr name="text_content" format="string" />
        <attr name="image_src" format="reference"/>
        <attr name="image_width" format="dimension"/>
        <attr name="image_height" format="dimension"/>
        <attr name="text_size" format="integer"/>
        <attr name="text_location" format="string"/>
    </declare-styleable>

</resources>